<div class="modal fade" id="productModal">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <div class="modal-header">
                <a href="#" class="close" data-dismiss="modal">&times;</a>
                <h3 class="modal-title">New Product</h3>
            </div>
            <div class="modal-body" id="employeeModalBody">
                <div class="sparkline12-graph">
                    <div class="input-knob-dial-wrap">
                        <div class="row">
                            <div class="col-lg-6">
                                <div class="col-lg-3">
                                    <div class="input-mask-title">
                                        <label>Name</label>
                                    </div>
                                </div>
                                <div class="col-lg-9">
                                    <div class="input-mark-inner mg-b-22">
                                        <input type="text" class="form-control" placeholder="Name" id="txtName">

                                    </div>
                                </div>
                            </div>
                            <div class="col-lg-6">
                                <div class="col-lg-3">
                                    <div class="input-mask-title">
                                        <label>Category</label>
                                    </div>
                                </div>
                                <div class="col-lg-9">
                                    <div class="floatleft">

                                        <select class="select2_Category " id="dropdownCategory"></select>
                                    </div>
                                </div>
                            </div>

                        </div>
                        <div class="row">
                            <div class="col-lg-6">
                                <div class="col-lg-3">
                                    <div class="input-mask-title">
                                        <label>Description</label>
                                    </div>
                                </div>
                                <div class="col-lg-9">
                                    <div class="input-mark-inner mg-b-22">
                                        <input type="text" class="form-control" placeholder="Description" id="txtDescription">

                                    </div>
                                </div>
                            </div>
                            <div class="col-lg-6">
                                <div class="col-lg-3">
                                    <div class="input-mask-title">
                                        <label>Supplier</label>
                                    </div>
                                </div>
                                <div class="col-lg-9">
                                    <div class="input-mark-inner mg-b-22">
                                        <div class="floatleft">

                                            <select class="select2_Supplier" id="dropdownSupplier"></select>
                                        </div>

                                    </div>
                                </div>
                            </div>

                        </div>
                        <div class="row">
                            <div class="col-lg-6">
                                <div class="col-lg-3">
                                    <div class="input-mask-title">
                                        <label>Quantity</label>
                                    </div>
                                </div>
                                <div class="col-lg-9">
                                    <div class="input-mark-inner mg-b-22">
                                        <input type="number" class="form-control" placeholder="" min="0" id="txtQuantity">

                                    </div>
                                </div>
                            </div>
                            <div class="col-lg-6">
                                <div class="col-lg-3">
                                    <div class="input-mask-title">
                                        <label>Status</label>
                                    </div>
                                </div>
                                <div class="col-lg-9">
                                    <div class=" mg-b-22">
                                        <button class="btn btn-xs floatleft">Active</button>

                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-lg-6">
                                <div class="col-lg-3">
                                    <div class="input-mask-title">
                                        <label>Price/Unit</label>
                                    </div>
                                </div>
                                <div class="col-lg-9">
                                    <div class="input-mark-inner mg-b-22">
                                        <input type="text" class="form-control" placeholder="" id="txtPrice">

                                    </div>
                                </div>
                            </div>

                        </div>

                        <div class="row">
                            <div class="col-lg-8">
                            </div>
                            <div class="col-lg-4 txt-align-r">
                                <a class="btn btn-success mg-5" href="#" onclick="InsertProduct()">Submit</a>
                                <a class="btn btn-default" href="#" data-dismiss="modal">Cancel</a>
                            </div>

                        </div>
                    </div>
                </div>
            </div>


        </div>

    </div>

</div>
